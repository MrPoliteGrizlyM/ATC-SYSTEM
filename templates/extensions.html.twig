{% extends '@SonataAdmin/CRUD/base_edit.html.twig' %}

{% block javascripts %}
    {{ parent() }}
    <script type="text/javascript">

      $(function() {

          $("#delete-image-avatar").on('click', function () {
              var id = $("#delete-image-avatar").attr('name');
              window.location.replace("/admin/delete/image/avatar/"+id);
          });

          $("#delete-image-map").on('click', function () {
              var id = $("#delete-image-map").attr('name');
              window.location.replace("/admin/delete/image/map/"+id);
          });

      });


    </script>


{% endblock %}